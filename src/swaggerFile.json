  "paths":{
    "/users":{
      "post":{
        "tags":["Users"],
        "summary": "Create a user",
        "descrption": "Create a new user",
        "requestBody": {
          "content":{
            "application/json":{
              "schema":{
                "type": "object",
                "properties":{
                  "name":{
                    "type":"string"
                  },
                  "email":{
                    "type":"string"
                  }
                },
                "example":{
                  "name": "Harry Potter",
                  "email": "harry@hotmail.com"
                }
              }
            }
          }
        },
        "responses":{
          "201":{
            "description":"Created a new user"
          },
          "400":{
            "description": "error a create new user"
          }
        }
      },
      "get":{
        "tags":["Users"],
        "summary": "List all Users",
        "description": "List all Users",
        "responses":{
          "200":{
            "description": "Success",
            "content":{
              "application/json":{
                "schema":{
                  "type":"array",
                  "items":{
                    "type":"object",
                    "example":[
                      {
                        "id": "jasdklliou434234u42o3dsdsdsu4",
                        "name": "Harry Potter",
                        "email": "harry@hotmail.com",
                        "created_at": "00/00/0000 00:00:00",
                        "updated_at": "11/11/1111 11:11:11",
                        "admin": false
                      },
                      {
                        "id": "jajko5647894u42o3u4",
                        "name": "Harry Potter",
                        "email": "harry@hotmail.com",
                        "created_at": "00/00/0000 00:00:00",
                        "updated_at": "11/11/1111 11:11:11",
                        "admin": true
                      }
                    ]
                  }
                }
              }
            }
          }
        }
      }
